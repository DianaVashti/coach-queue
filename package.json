    "express-session": "^1.14.2",
    "google-calendar": "^1.3.2",
    "knex": "^0.12.6",
    "material-ui": "^0.16.4",
    "morgan": "~1.7.0",
    "passport": "^0.3.2",
    "pg": "^6.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "serve-favicon": "~2.3.0",
    "webpack-dev-middleware": "^1.8.4"
  },
  "devDependencies": {
    "babel-cli": "~6.18.0",
