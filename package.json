    "babel-preset-latest": "~6.16.0",
    "babel-preset-react": "~6.16.0",
    "babel-runtime": "~6.20.0",
    "chai": "~3.5.0",
    "chai-datetime": "~1.4.1",
    "chai-http": "~3.0.0",
