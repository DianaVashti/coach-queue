{
  "name": "coach-que",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development ./node_modules/nodemon/bin/nodemon.js ./bin/www",
    "migrate:latest": "NODE_ENV=development knex migrate:latest --knexfile config/DBconfig.js",
    "migrate:latest:rollback": "knex migrate:rollback --knexfile config/DBconfig.js",
    "migrate:test": "NODE_ENV=test knex migrate:latest --knexfile config/DBconfig.js",
    "migrate:rollback:test": "NODE_ENV=test knex migrate:rollback --knexfile config/DBconfig.js",
    "test": "NODE_ENV=test mocha --timeout 10000 --recursive test/models/*.js",
    "seed:testdb": "knex seed:run --knexfile config/DBconfig.js",
    "webpack": "webpack --progress --colors --json | webpack-bundle-size-analyzer"
  },
