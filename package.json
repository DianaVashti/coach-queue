{
  "name": "coach-que",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development nodemon ./bin/www",
    "migrate:latest": "knex migrate:latest --knexfile config/DBconfig.js",
    "migrate:latest:rollback": "knex migrate:rollback --knexfile config/DBconfig.js",
    "migrate:test": "NODE_ENV=test knex migrate:latest --knexfile config/DBconfig.js",
    "test": "NODE_ENV=test mocha --timeout 10000 --recursive"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "express": "~4.14.0",
    "knex": "^0.12.6",
    "morgan": "~1.7.0",
    "pg": "^6.1.0",
    "pug": "~2.0.0-beta6",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-datetime": "^1.4.1",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  }
}
